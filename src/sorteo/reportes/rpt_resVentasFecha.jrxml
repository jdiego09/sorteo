	<subDataset name="tableDataset" uuid="f13e6d36-5148-4ecc-bbe3-3035def80980">
		<property name="net.sf.jasperreports.data.adapter" value="sorteo"/>
		<queryString>
			<![CDATA[]]>
		</queryString>
	</subDataset>
	<parameter name="tiposorteo" class="java.lang.Integer"/>
	<parameter name="fechaventa" class="java.sql.Date"/>
	<parameter name="numero" class="java.lang.Integer"/>
	<parameter name="descTipoSorteo" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT MAX(CONCAT(e.emp_nombre, ' ', x.suc_nombre)) descripcion,
    MAX(e.emp_logo) logo,
    MAX(COALESCE(e.emp_telefono, x.suc_telefono)) telefono,
    MAX(e.emp_cedjuridica) cedjuridica,
    MAX(COALESCE(e.emp_email, suc_email)) email,
    MAX(t.tso_descripcion) tiposorteo,
    s.sor_fecha fechasorteo,
    d.dfa_numero,
    d.dfa_monto apuesta
FROM
    sorteo.sor_detfactura d
        JOIN
    sorteo.sor_factura f ON d.dfa_codfac = f.fac_codigo
        JOIN
    sorteo.sor_sorteo s ON s.sor_codigo = f.fac_codsorteo
        JOIN
    sorteo.sor_tipoSorteo t ON t.tso_codigo = s.sor_tiposorteo 
        JOIN
    sorteo.sor_sucursal x ON x.suc_codigo = s.sor_codsucursal
        JOIN
    sorteo.sor_empresa e ON e.emp_codigo = x.suc_codempresa
    WHERE t.tso_codigo =  $P{tiposorteo} 
         AND f.fac_fecha =  $P{fechaventa} 
         AND d.dfa_numero = coalesce($P{numero},d.dfa_numero)
GROUP BY s.sor_fecha , d.dfa_numero, d.dfa_monto
ORDER BY s.sor_fecha , d.dfa_numero]]>
	</queryString>
